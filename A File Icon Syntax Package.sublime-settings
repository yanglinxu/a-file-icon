  /*
    // The list of packages with syntaxes which use the icon.
    // Note, the name of this setting should match the icon asset
    // name without extension. E.g. we have the `file_type_foo.png`
    // icon for Foo files, so the name is next:
    "file_type_foo": [
      {
        // The name of the syntax package. This setting is used
        // for information purposes only.
        "package_name": "Foo",

        // Where you can find all necessary information about
        // the syntax package. Indicates that syntax package is
        // maintained by community.This also means that you have to
        // install this package to see the right icon.
        "package_url": "https://packagecontrol.io/packages/Foo",

        // A comma-separated list of default scopes provided by
        // the syntaxes of the package.
        "package_scopes": "source.foo, text.foo.basic"
      }
    ],
  */

